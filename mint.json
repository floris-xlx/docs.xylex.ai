{
    "$schema": "https://mintlify.com/schema.json",
    "name": "Starter Kit",
    "logo": {
        "dark": "/logo/dark.svg",
        "light": "/logo/light.svg"
    },
    "favicon": "/favicon.svg",
    "colors": {
        "primary": "#9351d1",
        "light": "#9351d1",
        "dark": "#9351d1",
        "anchors": {
            "from": "#9351d1",
            "to": "#b78edd"
        }
    },
    "topbarLinks": [
        {
            "name": "Support",
            "url": "mailto:support@xylex.ai"
        }
    ],
    "topbarCtaButton": {
        "name": "Dashboard",
        "url": "https://app.xylex.ai"
    },
    "anchors": [
        {
            "name": "Documentation",
            "icon": "book-open-cover",
            "url": "https://nightly.docs.xylex.ai"
        },
        {
            "name": "Community",
            "icon": "slack",
            "url": "https://mintlify.com/community"
        },
        {
            "name": "Blog",
            "icon": "newspaper",
            "url": "https://mintlify.com/blog"
        }
    ],
    "navigation": [
        {
            "group": "Get Started",
            "pages": [
                "introduction",
                "quickstart",
                "development"
            ]
        },
        {
            "group": "Essentials",
            "pages": [
                "essentials/markdown",
                "essentials/code",
                "essentials/images",
                "essentials/settings",
                "essentials/navigation",
                "essentials/reusable-snippets"
            ]
        },
        {
            "group": "Authenticator API",
            "pages": [
                "pages/authenticator/verify_code_secret",
                "pages/authenticator/qr_code_svg",
                "pages/authenticator/generate_secret",
                "pages/authenticator/qr_code_url",
                "pages/authenticator/get_code_secret",
                "pages/authenticator/verify_code_user_id",
                "pages/authenticator/get_code_user_id"
            ]
        },
        {
            "group": "Organization API",
            "pages": [
                "pages/organization/organization_all_trades",
                "pages/organization/organization_available",
                "pages/organization/organization_create"
            ]
        },
        {
            "group": "Public API",
            "pages": [
                "pages/public/get_user_profile"
            ]
        },
        {
            "group": "Strategies API",
            "pages": [
                "pages/strategies/strategy_available",
                "pages/strategies/strategy_create",
                "pages/strategies/strategy_equity_pnl",
                "pages/strategies/strategy_metadata",
                "pages/strategies/strategy_statistics"
            ]
        },
        {
            "group": "Trades API",
            "pages": [
                "pages/trades/trade_metadata",
                "pages/trades/trade_status",
                "pages/trades/user_trade_metadata",
                "pages/trades/weekly_trades"
            ]
        },
        {
            "group": "User API",
            "pages": [
                "pages/user/organization",
                "pages/user/user_pfp",
                "pages/user/username_available",
                "pages/user/username_create"
            ]
        }
    ],
    "footerSocials": {
        "x": "https://discord.gg/xylex",
        "github": "https://github.com/xylex-ai"
    },
    "api": {
        "baseUrl": "https://api.v2.xylex.api",
        "playground": {
            "mode": "show"
        },
        "maintainOrder": true,
        "request": {
            "example": {
                "languages": [
                    "bash",
                    "javascript",
                    "python"
                ],
                "showOptionalParams": true
            }
        }
    }
}