{
    "$schema": "https://mintlify.com/schema.json",
    "name": "Starter Kit",
    "logo": {
        "dark": "/logo/dark.svg",
        "light": "/logo/light.svg"
    },
    "favicon": "/favicon.svg",
    "colors": {
        "primary": "#9351d1",
        "light": "#9351d1",
        "dark": "#9351d1",
        "anchors": {
            "from": "#9351d1",
            "to": "#b78edd"
        }
    },
    "topbarLinks": [
        {
            "name": "Support",
            "url": "mailto:support@xylex.ai"
        }
    ],
    "topbarCtaButton": {
        "name": "Dashboard",
        "url": "https://app.xylex.ai"
    },
    "anchors": [
        {
            "name": "Documentation",
            "icon": "book-open-cover",
            "url": "https://nightly.docs.xylex.ai"
        },
        {
            "name": "Community",
            "icon": "slack",
            "url": "https://mintlify.com/community"
        },
        {
            "name": "Blog",
            "icon": "newspaper",
            "url": "https://mintlify.com/blog"
        }
    ],
    "navigation": [
        {
            "group": "Get Started",
            "pages": [
                "introduction",
                "quickstart",
                "development"
            ]
        },
        {
            "group": "Essentials",
            "pages": [
                "essentials/markdown",
                "essentials/code",
                "essentials/images",
                "essentials/settings",
                "essentials/navigation",
                "essentials/reusable-snippets"
            ]
        },
        {
            "group": "Authenticator API",
            "pages": [
                "authenticator/generate_secret.rs",
                "authenticator/get_code.rs",
                "authenticator/secret_qr.rs",
                "authenticator/utils.rs",
                "authenticator/verify_code.rs"
            ]
        },
        {
            "group": "Organization API",
            "pages": []
        },
        {
            "group": "Public API",
            "pages": []
        },
        {
            "group": "Strategies API",
            "pages": []
        },
        {
            "group": "Trades API",
            "pages": []
        },
        {
            "group": "User API",
            "pages": []
        }
    ],
    "footerSocials": {
        "discord": "https://discord.gg/xylex",
        "github": "https://github.com/xylex-ai"
    },
    "api": {
        "baseUrl": "https://api.v2.xylex.api",
        "playground": {
            "mode": "show"
        },
        "maintainOrder": true,
        "request": {
            "example": {
                "languages": [
                    "bash",
                    "javascript",
                    "python"
                ],
                "showOptionalParams": true
            }
        }
    }
}